{
  "name": "my-money-manager",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "react": "^15.6.1",
    "react-dom": "^15.6.1",
    "react-scripts": "1.0.10",
    "request-promise": "^4.2.1"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "cp ./src/config.js ./src/config.bak.js && cp ./src/config-prod.js ./src/config.js && react-scripts build && mv ./src/config.bak.js ./src/config.js",
    "deploy": "aws s3 cp --recursive ./build s3://mymoneymanager.com",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject"
  }
}
